<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Contact_Number_Required</fullName>
    <active>true</active>
    <errorConditionFormula>OR( ISBLANK( Emergency_Contact_Number__c ) ,ISBLANK(  Parent_Mobile_Number__c  ))</errorConditionFormula>
    <errorMessage>Please enter both Emergency and Parent mobile numbers</errorMessage>
</ValidationRule>
